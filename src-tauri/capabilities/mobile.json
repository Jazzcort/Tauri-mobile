{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile-capability",
  "windows": [
    "main"
  ],
  "platforms": [
    "iOS",
    "android"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "window:allow-set-title",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "http:default"
  ]
}